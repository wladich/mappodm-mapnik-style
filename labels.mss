#cottage_labels[zoom>=13] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
}

#restricted_labels[zoom>=13] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
}

#poi[poi="town"][zoom >= 8] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
}

#poi[poi="village"][zoom >= 12] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
}

#poi[poi="label"][zoom >= 12] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
}

#poi[poi="railway_station"][zoom >= 12] {
    text-name: [name];
    text-face-name: "Arial Regular";
    text-size: 3mm;
    text-allow-overlap: true;
    text-dx: 3mm;
    text-dy: 3mm;
}

