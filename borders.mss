#fences[zoom>=13] {
    line-color: black;
    line-width: 0.5mm;
    ::hatches {
        line-color: black;
        line-width: 1mm;
        line-dasharray: 0.5mm, 0.5mm, 0.5mm, 3mm;
        line-offset: -0.7mm;
        
    }
}

#forest_contour[zoom>=13]{
    line-color: #008f00;
    line-width: 0.7mm;
    line-dasharray: 0.7mm, 1mm;
}






